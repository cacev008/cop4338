#include <stdio.h>
int main()
{
char c;
unsigned char uc;
short s;
unsigned short us;
int i;
unsigned int ui;
long l;
unsigned long ul;
long long ll;
unsigned long long ull;
float f;
double d;

printf("char=%c, uchar=%c,\nshort=%s, ushort=%s,\nint=%i, uint=%i,\nlong=%l, ulong=%lu, sizeof(c), sizeof(uc), sizeof(s), sizeof(us), sizeof(i), sizeof(ui), sizeof(l), sizeof(ul), sizeof(ll), sizeof(ull), sizeof(f), sizeof(double)");
return 0;
}
